#include "s32z270dc2_r52-pinctrl-common.dtsi"

&swt0 {
	status = "okay";
};

&emdio {
	pinctrl-0 = <&emdio_default>;
	pinctrl-names = "default";
	status = "okay";

	phy0: ethernet-phy@7 {
		compatible = "ethernet-phy";
		reg = <0x7>;
		status = "okay";
	};
};

&enetc_psi0 {
	local-mac-address = [00 00 00 01 02 00];
	pinctrl-0 = <&eth0_default>;
	pinctrl-names = "default";
	clock-frequency = <300000000>;
	phy-handle = <&phy0>;
	status = "okay";
};

&can0 {
	pinctrl-0 = <&can0_default>;
	pinctrl-names = "default";
	bus-speed = <125000>;
	sample-point = <875>;
	bus-speed-data = <1000000>;
	sample-point-data = <875>;
	status = "okay";
};

&can1 {
	pinctrl-0 = <&can1_default>;
	pinctrl-names = "default";
	bus-speed = <125000>;
	sample-point = <875>;
	bus-speed-data = <1000000>;
	sample-point-data = <875>;
};

&i3c2 {
	pinctrl-0 = <&i3c2_default>;
	pinctrl-names = "default";
	i2c-scl-hz = <400000>;
	i3c-od-scl-hz = <2500000>;
	i3c-scl-hz = <12500000>;
	status = "okay";

	eeprom0: eeprom@500000000000000010 {
		compatible = "atmel,at24";
		reg = <0x50 0x00 0x00000010>;
		status = "okay";
		size = <128>;
		pagesize = <8>;
		address-width = <8>;
		timeout = <5>;
	};
};
